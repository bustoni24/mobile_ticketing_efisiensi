FROM `armada` `t`
JOIN (select tr0.id as trip_id, tr0.armada_id, tr0.boarding_city, c1.name as nama_kota_asal, tr0.destination_city, c2.name as nama_kota_tujuan, tr0.hari_aktif from trip tr0 join (select id, name from city) c1 on c1.id=tr0.boarding_city join (select id, name from city) c2 on c2.id=tr0.destination_city) tr ON tr.armada_id=t.id
