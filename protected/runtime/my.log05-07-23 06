FROM `trip_agen` `t`
JOIN (select id as titik_id, titik_keberangkatan, province_id, city_id, terminal_id, is_boarding, is_drop_off, alamat from titik_keberangkatan where is_boarding=1) as boarding ON boarding.titik_id=t.titik_keberangkatan_id
JOIN (select id as titik_id, titik_keberangkatan, province_id, city_id, terminal_id, is_boarding, is_drop_off, alamat from titik_keberangkatan where is_drop_off=1) as drop_off ON drop_off.titik_id=t.titik_keberangkatan_id
JOIN `city` `c` ON c.id=t.destination_city
WHERE boarding.province_id=:boardingProvince AND c.province_id=:dropProvince
GROUP BY `t`.`jam_berangkat`, `t`.`titik_keberangkatan_id`
ORDER BY `t`.`jam_berangkat` ASC. Bound with :boardingProvince='33', :dropProvince='34'.
