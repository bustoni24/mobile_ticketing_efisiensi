		FROM `presensi` t 
		LEFT JOIN (SELECT sdm_id, scan_date, min(scan_date) as scan_masuk FROM presensi GROUP By sdm_id, date(scan_date)) masuk ON masuk.sdm_id = t.sdm_id AND date(masuk.scan_date) = date(t.scan_date)
